<div id="<%= dom_id message %>">
  <p>
    <strong>Group:</strong>
    <%= message.group_id %>
  </p>

  <p>
    <strong>Sender:</strong>
    <%= message.sender_id %>
  </p>

  <p>
    <strong>Audio:</strong>
    <%= message.audio %>
  </p>

  <p>
    <strong>Video:</strong>
    <%= message.video %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= message.body %>
  </p>

  <p>
    <strong>Original language:</strong>
    <%= message.original_language_id %>
  </p>

</div>
